/* Number 7 of lab 1 */
#include <stdio.h> 

void main(){

int n, i, passing, failing, grade;

printf("Enter the number of grades: ");
scanf("%d", &n);

for (i = 1; i <= n; i++){
	printf("Enter an integer grade: ");
	scanf("%d", &grade);

	if(grade >= 60)
	   passing ++;
	else
	   failing ++;
}//for loop to ask for different grade values

printf("The number of passing grades is %d\nThe number of failing grades is %d\n", passing, failing);

}//main method
